{
  "name": "chaindler",
  "version": "0.1.2",
  "description": "A simple request handler (controller/middleware) chainer",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "cleanup": "rimraf dist && rimraf coverage && rimraf .nyc_output && rimraf docs",
    "lint": "eslint ./src --ext .ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "docs": "typedoc --out docs --mode modules --tsconfig tsconfig.json src && touch docs/.nojekyll",
    "docs:deploy": "npm run docs && gh-pages --dotfiles --dist docs",
    "test": "mocha --require ts-node/register --require source-map-support/register --file test/index.ts test/**/*.test.ts --exit",
    "test:coverage": "nyc --reporter lcov --include src npm run test",
    "test:coveralls": "cat coverage/lcov.info | coveralls",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zishone/chaindler.git"
  },
  "keywords": [
    "chain",
    "chainer",
    "handler",
    "controller",
    "middleware",
    "compose"
  ],
  "author": "Zishran Garces <zishgarces@gmail.com> (https://github.com/zishone)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zishone/chaindler/issues"
  },
  "homepage": "https://github.com/zishone/chaindler#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.32",
    "@types/sinon": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.13.0",
    "gh-pages": "^2.2.0",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.1",
    "typedoc": "^0.17.6",
    "typescript": "^3.9.7"
  }
}
